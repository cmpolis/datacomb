{
  "name": "datacomb",
  "version": "0.0.1",
  "description": "An interactive tool for exploring large, tabular datasets.",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && mocha test/*-test.js",
    "serve": "./node_modules/serve/bin/serve -p 5050",
    "build": "./node_modules/node-sass/bin/node-sass css -o build/css && ./node_modules/browserify/bin/cmd.js js/index.js -o build/datacomb.js -s Datacomb -t stringify && echo 'Done building.'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cmpolis/datacomb"
  },
  "keywords": [
    "data",
    "d3",
    "visualization",
    "datavis"
  ],
  "author": "Chris Polis, @ChrisPolis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cmpolis/datacomb/issues"
  },
  "homepage": "https://github.com/cmpolis/datacomb",
  "dependencies": {
    "autoprefixer": "^6.0.2",
    "browserify": "^11.0.1",
    "canvas-dpi-scaler": "0.0.3",
    "chai": "^3.2.0",
    "d3": "^3.5.6",
    "jStat": "^1.4.1",
    "lodash": "^3.10.1",
    "mocha": "^2.2.5",
    "node-sass": "^3.2.0",
    "postcss-cli": "^2.1.0",
    "ractive": "^0.7.3",
    "serve": "^1.4.0",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "smart-table-scroll": "0.2.0",
    "stringify": "^3.1.0"
  }
}
